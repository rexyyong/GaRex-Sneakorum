  },
  "homepage": "https://github.com/subtirelumihail/react-fullpage#readme",
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.19.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
