{
  "name": "react-fullpage",
  "version": "0.1.1",
  "description": "An implementation of fullpage.js in react",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "./node_modules/babel/bin/babel.js src --out-dir dist",
